<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" cache="true" prefHeight="315.0" prefWidth="303.0" stylesheets="@../../../resources/main.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="comp421.group46.controller.RegistrationController">
   <children>
      <VBox alignment="CENTER" layoutX="7.0" layoutY="6.0" spacing="5.0">
         <children>
            <Label text="REGISTRATION" textAlignment="CENTER">
               <font>
                  <Font name="Shonar Bangla" size="48.0" />
               </font>
            </Label>
            <HBox>
               <children>
                  <Label prefHeight="27.0" prefWidth="100.0" text="Customer ID : ">
                     <font>
                        <Font name="Segoe UI Light" size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="customerIDtextField" editable="false" prefHeight="25.0" prefWidth="120.0" />
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label prefHeight="27.0" prefWidth="99.0" text="First Name    :">
                     <font>
                        <Font name="Segoe UI Light" size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="firstNameTextField" editable="false" prefHeight="25.0" prefWidth="120.0" promptText="Your first name" />
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label prefHeight="27.0" prefWidth="100.0" text="Last Name    :">
                     <font>
                        <Font name="Segoe UI Light" size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="lastNameTextField" prefHeight="25.0" prefWidth="120.0" promptText="Your last name" />
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label prefHeight="27.0" prefWidth="100.0" text="Email            :">
                     <font>
                        <Font name="Segoe UI Light" size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="emailTextField" prefHeight="25.0" prefWidth="120.0" promptText="Your email" />
               </children>
            </HBox>
            <VBox alignment="CENTER">
               <children>
                  <Label text="Address">
                     <font>
                        <Font name="Segoe UI Semibold" size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="addressTextField" alignment="CENTER" promptText="Your address, e.g. 58 Dog Lane" />
               </children>
            </VBox>
            <HBox alignment="CENTER" prefHeight="53.0" prefWidth="181.0" spacing="30.0">
               <children>
                  <Button alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleConfirm" prefWidth="75.0" text="Register" />
                  <Button alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleCancel" prefWidth="75.0" text="Cancel" />
               </children>
               <padding>
                  <Insets top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
